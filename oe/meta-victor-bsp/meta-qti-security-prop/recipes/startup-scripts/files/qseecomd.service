[Unit]
Description=Qseecomd Service
SourcePath=/etc/initscripts/qseecomd

[Service]
Restart=no
RemainAfterExit=yes
ExecStart=/etc/initscripts/qseecomd start
ExecStop=/etc/initscripts/qseecomd stop

[Install]
WantedBy=multi-user.target
