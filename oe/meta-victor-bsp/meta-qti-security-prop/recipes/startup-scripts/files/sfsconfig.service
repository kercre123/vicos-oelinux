[Unit]
Description=sfs configuration Service
SourcePath=/etc/initscripts/sfs_config
After=qseecomd.service

[Service]
Restart=no
RemainAfterExit=yes
ExecStart=/etc/initscripts/sfs_config
ExecStop=/etc/initscripts/sfs_config

[Install]
WantedBy=multi-user.target
