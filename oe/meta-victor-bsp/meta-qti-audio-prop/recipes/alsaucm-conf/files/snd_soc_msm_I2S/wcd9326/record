SectionVerb {
        EnableSequence [
                cdev "hw:0"
                cset "name='MultiMedia1 Mixer QUAT_MI2S_TX' 1"
        ]
        DisableSequence [
                cdev "hw:0"
                cset "name='MultiMedia1 Mixer QUAT_MI2S_TX' 0"
        ]
}

#DMIC0
SectionDevice."PrimaryMIC" {
        EnableSequence [
                cdev "hw:0"
                cset "name='AIF1_CAP Mixer SLIM TX6' 1"
                cset "name='MI2S_TX Channels' 0"
                cset "name='SLIM TX6 MUX' 2"
                cset "name='ADC MUX6' 0"
                cset "name='DMIC MUX6' 1"
                cset "name='IIR0 INP0 MUX' 7"
        ]
        DisableSequence [
                cdev "hw:0"
                cset "name='AIF1_CAP Mixer SLIM TX6' 0"
                cset "name='MI2S_TX Channels' 0"
                cset "name='SLIM TX6 MUX' 0"
                cset "name='ADC MUX6' 0"
                cset "name='DMIC MUX6' 0"
                cset "name='IIR0 INP0 MUX' 0"
        ]
}

