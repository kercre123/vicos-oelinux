SectionDevice."QuadMIC" {
        EnableSequence [
                cdev "hw:0"
                cset "name='AIF1_CAP Mixer SLIM TX6' 1"
                cset "name='AIF1_CAP Mixer SLIM TX7' 1"
                cset "name='AIF1_CAP Mixer SLIM TX8' 1"
                cset "name='MI2S_TX Channels' 2"
                cset "name='SLIM TX6 MUX' 2"
                cset "name='ADC MUX6' 0"
                cset "name='DMIC MUX6' 4"
                cset "name='SLIM TX7 MUX' 2"
                cset "name='ADC MUX7' 0"
                cset "name='DMIC MUX7' 1"
                cset "name='SLIM TX8 MUX' 2"
                cset "name='ADC MUX8' 0"
                cset "name='DMIC MUX8' 3"
        ]
        DisableSequence [
                cdev "hw:0"
                cset "name='AIF1_CAP Mixer SLIM TX6' 0"
                cset "name='AIF1_CAP Mixer SLIM TX7' 0"
                cset "name='AIF1_CAP Mixer SLIM TX8' 0"
                cset "name='MI2S_TX Channels' 0"
                cset "name='SLIM TX6 MUX' 0"
                cset "name='ADC MUX6' 0"
                cset "name='DMIC MUX6' 0"
                cset "name='SLIM TX7 MUX' 0"
                cset "name='ADC MUX7' 0"
                cset "name='DMIC MUX7' 0"
                cset "name='SLIM TX8 MUX' 0"
                cset "name='ADC MUX8' 0"
                cset "name='DMIC MUX8' 0"
        ]
}
SectionDevice."Headset" {
        EnableSequence [
                cdev "hw:0"
                cset "name='MI2S_TX Channels' 0"
                cset "name='AIF1_CAP Mixer SLIM TX6' 1"
                cset "name='SLIM TX6 MUX' 2"
                cset "name='ADC MUX6' 1"
                cset "name='AMIC MUX6' 2"
                cset "name='DEC6 Volume' 84"
        ]
        DisableSequence [
                cdev "hw:0"
                cset "name='MI2S_TX Channels' 1"
                cset "name='AIF1_CAP Mixer SLIM TX6' 0"
                cset "name='SLIM TX6 MUX' 0"
                cset "name='ADC MUX6' 0"
                cset "name='AMIC MUX6' 0"
                cset "name='DEC6 Volume' 84"
        ]
}
