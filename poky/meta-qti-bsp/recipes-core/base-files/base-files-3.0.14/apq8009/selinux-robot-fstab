# Anki fstab

rootfs               /               auto      defaults              1  1
proc                 /proc           proc      defaults              0  0
devpts               /dev/pts        devpts    mode=0620,gid=5       0  0
usbdevfs             /proc/bus/usb   usbdevfs  noauto                0  0
tmpfs                /media/ram      tmpfs     defaults,noauto       0  0

PARTLABEL=userdata   /data           auto      defaults              0  2
PARTLABEL=modem      /firmware       vfat      defaults              0  0
PARTLABEL=persist    /persist        auto      defaults,noauto       0  0
PARTLABEL=cache      /cache          auto      defaults,noauto       0  0
